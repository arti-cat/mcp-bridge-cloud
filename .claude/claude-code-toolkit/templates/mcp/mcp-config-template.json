{
  "mcpServers": {
    "http-server-example": {
      "transport": "http",
      "url": "https://api.example.com/mcp",
      "headers": {
        "Authorization": "Bearer ${API_TOKEN}"
      }
    },
    "stdio-server-example": {
      "transport": "stdio",
      "command": "/path/to/mcp-server",
      "args": ["--option", "value"],
      "env": {
        "API_KEY": "${API_KEY}",
        "ENV": "production"
      }
    },
    "sse-server-example": {
      "transport": "sse",
      "url": "https://sse.example.com/mcp/events"
    }
  }
}
